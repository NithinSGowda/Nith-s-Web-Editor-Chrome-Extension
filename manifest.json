{
    "manifest_version":2,
    "name":"Nith's Web Editor",
    "version":"1.0.2",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "css":["classicStyle.css"]
        }
    ],
    "icons":{"128":"bitmoji.png"},
    "browser_action":{
        "default_icon":"mainlogo.png",
        "default_popup":"extIndex.html"
    },
    "permissions":["activeTab"]
}